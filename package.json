{
  "name": "navi_bot",
  "version": "1.0.0",
  "description": "Navi's existence in slack",
  "main": "./build/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-babel": "babel src/ -d build/ -s",
    "watch-babel": "babel -w src/ -d build/ -s",
    "build-css": "node-sass src/app/scss/ -d -o build/public/css",
    "watch-css": "node-sass -w src/app/scss/ -d -o build/public/css",
    "precompile": "babel src/ -d build -s && node-sass src/app/scss/ -d -o build/public/css && sequelize db:migrate",
    "prepare-production": "sequelize db:migrate --env=production && npm update && pm2 stop server && pm2 start server",
    "start": "node ./build/server.js",
    "start-dev": "node ./build/dev_slackbot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinsuh/navi_bot.git"
  },
  "author": "Kevin Suh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevinsuh/navi_bot/issues"
  },
  "homepage": "https://github.com/kevinsuh/navi_bot",
  "dependencies": {
    "body-parser": "^1.15.1",
    "botkit": "https://github.com/kevinsuh/botkit.git",
    "botkit-middleware-witai": "https://github.com/kevinsuh/botkit-middleware-witai.git",
    "cron": "^1.1.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "pg": "^5.0.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.72.0",
    "sequelize": "^3.23.3",
    "sequelize-cli": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "node-sass": "^3.7.0"
  }
}
